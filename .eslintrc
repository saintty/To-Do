{
  "extends": ["airbnb-base"],
  "env": {
    "browser": true
  },
  "rules": {
    "quotes": ["error", "double"],
    "no-console": "off",
    "arrow-body-style": ["error", "always"],
    "comma-dangle": ["error", "only-multiline"],
    "no-use-before-define": ["error", {
      "function": false,
      "classes": true,
      "variables": true,
      "allowNamedExports": false
    }]
  }
}
